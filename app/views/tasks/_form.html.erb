<%= form_for(task) do |f| %>
  <% if task.errors.any? %>
    <div id="error_axplanation">
      <ul>
        <% task.errors.full_messages.each do |task| %>
        <li><%= task %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  
  <%= f.label :status, "状態" %>
  <%= f.text_field :status %>
  
  <%= f.label :conteent, "タスク" %>
  <%= f.text_field :content %>
  
  <%= f.submit "投稿" %>
<% end %>
  